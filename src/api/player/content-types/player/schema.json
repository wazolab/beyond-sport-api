{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "birthdate": {
      "type": "string",
      "required": false,
      "regex": "(\\d{2})(|-|\\/|)(\\d{2})(|-|\\/|)(\\d{4})"
    },
    "size": {
      "type": "integer",
      "max": 300,
      "min": 0
    },
    "weight": {
      "type": "integer",
      "max": 300
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "club": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::club.club",
      "inversedBy": "players"
    }
  }
}

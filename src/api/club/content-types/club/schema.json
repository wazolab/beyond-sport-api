{
  "kind": "collectionType",
  "collectionName": "clubs",
  "info": {
    "singularName": "club",
    "pluralName": "clubs",
    "displayName": "Club",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "city": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "club"
    },
    "partners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partner.partner",
      "mappedBy": "club"
    },
    "players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player.player",
      "mappedBy": "club"
    },
    "primaryColor": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true,
      "customField": "plugin::color-picker.color"
    },
    "secondaryColor": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true,
      "customField": "plugin::color-picker.color"
    }
  }
}
